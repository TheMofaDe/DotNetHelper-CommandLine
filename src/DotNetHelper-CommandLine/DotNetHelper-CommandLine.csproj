<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net452;netstandard2.0;net45</TargetFrameworks>
    <RootNamespace>DotNetHelper_CommandLine</RootNamespace>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Authors>Joseph McNeal Jr</Authors>
    <Company>Joseph McNeal Jr</Company>
    <Description>A command-line helper class that makes it easy to run commands.</Description>
    <PackageProjectUrl>https://github.com/TheMofaDe/DotNetHelper-CommandLine</PackageProjectUrl>
    <PackageIconUrl>https://github.com/TheMofaDe/DotNetHelper-CommandLine</PackageIconUrl>
    <RepositoryUrl>https://github.com/TheMofaDe/DotNetHelper-CommandLine</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Command prompt line CommandPrompt</PackageTags>
    <PackageReleaseNotes>initial release</PackageReleaseNotes>
  </PropertyGroup>

</Project>
